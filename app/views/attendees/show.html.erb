<div class="page-header">
  <h3>Attendee #<%= @attendee.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @attendee.user_id %></dd>

      <dt>Party</dt>
      <dd><%= @attendee.party_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/attendees" class="btn btn-primary">
        Back
      </a>
      <a href="/attendees/<%= @attendee.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_attendee/<%= @attendee.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
